<%- include('partials/navbar.ejs') %>

    <div class="container">
        <h1>Do you want to delete book <%= book.title %>
        </h1>
        <form action="/delete/<%= book.bookId %>" method="post" style="display: inline-block; margin: 10px">
            <button type="submit">Yes</button>
        </form>
        <form action="/books/<%= book.bookId %>" method="get" style="display: inline-block">
            <button type="submit">No</button>
        </form>
    </div>